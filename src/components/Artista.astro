---
interface Props {
    artist: string;
    imgURL: string;
}
const { artist, imgURL } = Astro.props;
---

<div class="flex flex-col items-center grow rounded-x1 gap-2.5">
    <div>
        <img src={imgURL} alt="" class="w-3xs h-64 object-cover rounded-2xl" />
    </div>
    <div class="flex flex-col items-center bg-amber-50 rounded-full px-7">
        <h1 class="font-bold text-xl">{artist}</h1>
    </div>
</div>
